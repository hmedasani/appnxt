{
  "name": "appnxt",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install && yarn husky-commitmsg && yarn husky-precommit && yarn husky-prepush",
    "husky-commitmsg": "yarn husky add .husky/commit-msg 'yarn commitlint --edit $1'",
    "husky-precommit": "npx husky add .husky/pre-commit \"yarn lint\"",
    "husky-prepush": "npx husky add .husky/pre-push \"yarn build\"",
    "lint": "next lint",
    "format": "prettier --write ."
  },
  "dependencies": {
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/node": "17.0.34",
    "@types/prettier": "^2.6.1",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.6",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "typescript": "4.6.4"
  }
}
